{
    "name": "baijian/ahocorasick",
    "type": "library",
    "description": "AhoCorasick in PHP",
    "version": "0.0.1",
    "keywords": ["ahocorasick", "baijian"],
    "license": "MIT",
    "authors": [
        {
            "name": "baijian",
            "email": "jian.baij@gmail.com",
            "homepage": "http://joinjoy.me",
            "role": "Learner"
        }
    ],
    "homepage": "http://github.com/baijian/AhoCorasick-PHP",
    "autoload": {
        "psr-4": {
            "Baijian\\Algorithm\\" : "src/"
        }
    },
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/baijian/AhoCorasick-PHP"
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload --optimize",
            "php index.php"
        ],
        "post-update-cmd": [
            "composer dump-autoload --optimize",
            "php index.php"
        ]
    }
}
